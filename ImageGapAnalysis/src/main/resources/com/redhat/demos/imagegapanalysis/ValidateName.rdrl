package com.redhat.demos.imagegapanalysis;

import java.lang.Number;

rule "ValidateName"
	dialect "mvel"
	ruleflow-group "validationImage"
	when
		MovieEpisodeRequest( name == "test" )
	then
		ValidationError error = new ValidationError();
		error.setCause( "There has to be a title for this movie or episode" );
		insert( error );
		System.out.println("movie or episode is missing. Execute Rule: "  + drools.getRule().getName() );
end
